//REQ.: and,not

/**
 * 16-bit bitwise And:
 * for i = 0..15: out[i] = (a[i] and b[i])
 */

CHIP And8vs1sep {
	IN a[8], b;
	OUT out0, out1, out2, out3, out4, out5, out6, out7;

	PARTS:
	And(a=a[0], b=b, out=out0);
	And(a=a[1], b=b, out=out1);
	And(a=a[2], b=b, out=out2);
	And(a=a[3], b=b, out=out3);
	And(a=a[4], b=b, out=out4);
	And(a=a[5], b=b, out=out5);
	And(a=a[6], b=b, out=out6);
	And(a=a[7], b=b, out=out7);

}